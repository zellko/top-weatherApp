{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
       "prefer-destructuring": "off",
       "prefer-const": "off",
       "no-plusplus": "off"
    }
}
